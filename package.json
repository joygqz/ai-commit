{
  "publisher": "joygqz",
  "name": "commit-genie",
  "displayName": "Commit Genie",
  "version": "2.2.11",
  "packageManager": "pnpm@10.17.1",
  "description": "AI-powered code review and commit message generator",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/joygqz/commit-genie#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joygqz/commit-genie.git"
  },
  "bugs": {
    "url": "https://github.com/joygqz/commit-genie/issues"
  },
  "keywords": [
    "提交信息",
    "代码审查",
    "智能助手",
    "自动化",
    "语义提交",
    "生产力工具",
    "代码生成",
    "代码助手",
    "代码质量",
    "代码管理",
    "代码规范",
    "git",
    "commit",
    "commit message",
    "code review",
    "ai",
    "deepseek",
    "openai",
    "chatgpt",
    "conventional commits",
    "scm",
    "automation",
    "productivity",
    "developer tools",
    "code quality",
    "gitmoji",
    "llm",
    "code analysis",
    "pre-commit",
    "version control"
  ],
  "categories": [
    "SCM Providers",
    "AI",
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "./images/logo.png",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "dist/*",
    "images/*",
    "l10n/*.json",
    "package.nls*.json"
  ],
  "l10n": "./l10n",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Commit Genie",
        "command": "commit-genie.reviewAndCommit",
        "title": "%command.reviewAndCommit.title%",
        "icon": {
          "light": "images/logo.svg",
          "dark": "images/logo.svg"
        }
      },
      {
        "category": "Commit Genie",
        "command": "commit-genie.selectAvailableModel",
        "title": "%command.selectAvailableModel.title%"
      },
      {
        "category": "Commit Genie",
        "command": "commit-genie.showTokenStats",
        "title": "%command.showTokenStats.title%"
      },
      {
        "category": "Commit Genie",
        "command": "commit-genie.resetTokenStats",
        "title": "%command.resetTokenStats.title%"
      }
    ],
    "configuration": {
      "title": "Commit Genie",
      "properties": {
        "commit-genie.service.baseURL": {
          "default": "https://api.deepseek.com",
          "markdownDescription": "%configuration.service.baseURL.description%",
          "type": "string",
          "order": 0
        },
        "commit-genie.service.apiKey": {
          "default": "",
          "markdownDescription": "%configuration.service.apiKey.description%",
          "type": "string",
          "order": 1
        },
        "commit-genie.service.model": {
          "default": "deepseek-chat",
          "markdownDescription": "%configuration.service.model.description%",
          "type": "string",
          "order": 2
        },
        "commit-genie.format.outputLanguage": {
          "default": "简体中文",
          "markdownDescription": "%configuration.format.outputLanguage.description%",
          "enum": [
            "简体中文",
            "繁體中文",
            "にほんご",
            "한국어",
            "česky",
            "Deutsch",
            "française",
            "italiano",
            "Nederlands",
            "português",
            "tiếng Việt",
            "English",
            "español",
            "Svenska",
            "русский",
            "bahasa",
            "Polski",
            "Türkçe",
            "ไทย"
          ],
          "type": "string",
          "order": 10
        },
        "commit-genie.review.customPrompt": {
          "default": "",
          "markdownDescription": "%configuration.review.customPrompt.description%",
          "type": "string",
          "editPresentation": "multilineText",
          "order": 20
        },
        "commit-genie.commit.enableEmojiPrefix": {
          "default": false,
          "markdownDescription": "%configuration.commit.enableEmojiPrefix.description%",
          "type": "boolean",
          "order": 30
        },
        "commit-genie.commit.customPrompt": {
          "default": "",
          "markdownDescription": "%configuration.commit.customPrompt.description%",
          "type": "string",
          "editPresentation": "multilineText",
          "order": 31
        }
      }
    },
    "menus": {
      "scm/title": [
        {
          "command": "commit-genie.reviewAndCommit",
          "group": "navigation",
          "when": "scmProvider === git"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ext:package": "vsce package",
    "ext:publish": "vsce publish",
    "release": "bumpp -x \"pnpm run changelog\" -a"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.18.8",
    "@types/vscode": "^1.75.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "npm-run-all": "^4.1.5",
    "openai": "^6.1.0",
    "simple-git": "^3.28.0",
    "typescript": "^5.9.3"
  }
}
