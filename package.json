{
  "publisher": "joygqz",
  "name": "commit-genie",
  "displayName": "Commit Genie",
  "version": "1.0.2",
  "packageManager": "pnpm@10.17.1",
  "description": "AI Commit Message Generator",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/joygqz/commit-genie#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joygqz/commit-genie.git"
  },
  "bugs": {
    "url": "https://github.com/joygqz/commit-genie/issues"
  },
  "keywords": [],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "./images/logo.png",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "dist/*",
    "images/*",
    "l10n/*.json",
    "package.nls*.json"
  ],
  "l10n": "./l10n",
  "engines": {
    "vscode": "^1.75.0"
  },
  "contributes": {
    "commands": [
      {
        "category": "Commit Genie",
        "command": "commit-genie.generateCommitMessage",
        "title": "%command.generateCommitMessage.title%",
        "icon": {
          "light": "images/logo.svg",
          "dark": "images/logo.svg"
        }
      },
      {
        "category": "Commit Genie",
        "command": "commit-genie.selectAvailableModel",
        "title": "%command.selectAvailableModel.title%"
      }
    ],
    "configuration": {
      "title": "Commit Genie",
      "type": "object",
      "properties": {
        "commit-genie.service.baseURL": {
          "default": "https://api.deepseek.com",
          "markdownDescription": "%configuration.service.baseURL.description%",
          "type": "string",
          "order": 0
        },
        "commit-genie.service.apiKey": {
          "default": "",
          "markdownDescription": "%configuration.service.apiKey.description%",
          "type": "string",
          "order": 1
        },
        "commit-genie.service.model": {
          "default": "deepseek-chat",
          "markdownDescription": "%configuration.service.model.description%",
          "type": "string",
          "order": 2
        },
        "commit-genie.format.commitMessageLanguage": {
          "default": "Simplified Chinese",
          "markdownDescription": "%configuration.format.commitMessageLanguage.description%",
          "enum": [
            "Simplified Chinese",
            "Traditional Chinese",
            "Japanese",
            "Korean",
            "Czech",
            "German",
            "French",
            "Italian",
            "Dutch",
            "Portuguese",
            "Vietnamese",
            "English",
            "Spanish",
            "Swedish",
            "Russian",
            "Bahasa",
            "Polish",
            "Turkish",
            "Thai"
          ],
          "enumDescriptions": [
            "简体中文",
            "繁體中文",
            "にほんご",
            "한국어",
            "česky",
            "Deutsch",
            "française",
            "italiano",
            "Nederlands",
            "português",
            "tiếng Việt",
            "english",
            "español",
            "Svenska",
            "русский",
            "bahasa",
            "Polski",
            "Turkish",
            "ไทย"
          ],
          "type": "string",
          "order": 3
        },
        "commit-genie.format.enableEmojiPrefix": {
          "default": false,
          "markdownDescription": "%configuration.format.enableEmojiPrefix.description%",
          "type": "boolean",
          "order": 4
        },
        "commit-genie.format.customPrompt": {
          "default": "",
          "markdownDescription": "%configuration.format.customPrompt.description%",
          "type": "string",
          "editPresentation": "multilineText",
          "order": 5
        },
        "commit-genie.debug.enableLogging": {
          "default": true,
          "markdownDescription": "%configuration.debug.enableLogging.description%",
          "type": "boolean",
          "order": 6
        },
        "commit-genie.debug.logLevel": {
          "default": "warn",
          "markdownDescription": "%configuration.debug.logLevel.description%",
          "type": "string",
          "enum": [
            "debug",
            "info",
            "warn",
            "error"
          ],
          "enumDescriptions": [
            "Debug: Show all logs (for development)",
            "Info: Show info, warnings and errors",
            "Warn: Show warnings and errors (recommended)",
            "Error: Show only errors"
          ],
          "order": 7
        }
      }
    },
    "menus": {
      "scm/title": [
        {
          "command": "commit-genie.generateCommitMessage",
          "group": "navigation",
          "when": "scmProvider === git"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ext:package": "vsce package",
    "ext:publish": "vsce publish",
    "release": "bumpp -x \"pnpm run changelog\" -a"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.18.8",
    "@types/vscode": "^1.73.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "npm-run-all": "^4.1.5",
    "openai": "^6.1.0",
    "simple-git": "^3.28.0",
    "typescript": "^5.9.3"
  }
}
