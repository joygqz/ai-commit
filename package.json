{
  "publisher": "joygqz",
  "name": "commit-genie",
  "displayName": "Commit Genie",
  "version": "0.2.3",
  "packageManager": "pnpm@10.4.1",
  "description": "AI Commit Message Generator",
  "author": "joygqz",
  "license": "MIT",
  "homepage": "https://github.com/joygqz/commit-genie#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joygqz/commit-genie.git"
  },
  "bugs": {
    "url": "https://github.com/joygqz/commit-genie/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "images/icon.png",
  "files": [
    "LICENSE",
    "dist/*",
    "images/*"
  ],
  "engines": {
    "vscode": "^1.77.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "commit-genie.generateCommitMessage",
        "title": "Generate Commit Message",
        "category": "Commit Genie",
        "icon": {
          "dark": "images/icon.svg",
          "light": "images/icon.svg"
        }
      },
      {
        "command": "commit-genie.selectAvailableModel",
        "title": "Select Available Model",
        "category": "Commit Genie"
      }
    ],
    "configuration": {
      "title": "Commit Genie",
      "type": "object",
      "properties": {
        "commit-genie.apiKey": {
          "default": "",
          "description": "API key for authentication with your AI service provider.",
          "type": "string",
          "order": 0
        },
        "commit-genie.baseURL": {
          "default": "https://api.deepseek.com",
          "description": "Base URL of the AI service API endpoint.",
          "type": "string",
          "order": 1
        },
        "commit-genie.model": {
          "default": "deepseek-chat",
          "description": "AI model to use for generating commit messages. Enter manually or run \"Commit Genie: Select Available Models\" to choose from available options.",
          "type": "string",
          "order": 2
        },
        "commit-genie.commitMessageLanguage": {
          "default": "Simplified Chinese",
          "description": "Language for generated commit messages.",
          "enum": [
            "Simplified Chinese",
            "Traditional Chinese",
            "Japanese",
            "Korean",
            "Czech",
            "German",
            "French",
            "Italian",
            "Dutch",
            "Portuguese",
            "Vietnamese",
            "English",
            "Spanish",
            "Swedish",
            "Russian",
            "Bahasa",
            "Polish",
            "Turkish",
            "Thai"
          ],
          "enumDescriptions": [
            "简体中文",
            "繁體中文",
            "にほんご",
            "한국어",
            "česky",
            "Deutsch",
            "française",
            "italiano",
            "Nederlands",
            "português",
            "tiếng Việt",
            "english",
            "español",
            "Svenska",
            "русский",
            "bahasa",
            "Polski",
            "Turkish",
            "ไทย"
          ],
          "type": "string",
          "order": 3
        },
        "commit-genie.enableEmojiPrefix": {
          "default": false,
          "description": "Enable emoji prefix.",
          "type": "boolean",
          "order": 4
        }
      }
    },
    "menus": {
      "scm/title": [
        {
          "command": "commit-genie.generateCommitMessage",
          "group": "navigation",
          "when": "scmProvider == git"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp",
    "ext:package": "vsce package --no-dependencies",
    "ext:publish": "vsxpub --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.18.6",
    "@types/vscode": "^1.77.0",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "esno": "^4.8.0",
    "openai": "^5.23.1",
    "ovsx": "^0.10.5",
    "pnpm": "^10.17.1",
    "reactive-vscode": "^0.2.22",
    "simple-git": "^3.28.0",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vscode-ext-gen": "^1.3.0",
    "vsxpub": "^0.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vscode/vsce-sign",
      "esbuild",
      "keytar",
      "rolldown",
      "vsxpub"
    ]
  }
}
